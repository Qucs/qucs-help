#: ../../source/programs.rst:56
msgid "``qucsedit [-r] [file]`` (r = read-only)"
msgstr ""

# 72e1db7036374329a2f03aca4b28088f
#: ../../source/programs.rst:58
msgid "``qucshelp`` (no arguments)"
msgstr ""

